{
  "version": "0.1",
  "prefixes": {
    "ext": "http://mu.semte.ch/vocabularies/ext/",
    "nfo": "http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#",
    "dct": "http://purl.org/dc/terms/",
    "dbpedia": "http://dbpedia.org/ontology/",
    "nie": "http://www.semanticdesktop.org/ontologies/2007/01/19/nie#"
  },
  "resources": {
    "regulatory-statements": {
      "name": "regulatory-statement",
      "class": "ext:Reglement",
      "attributes": {
        "folder": {
          "type": "string",
          "predicate": "ext:regulatoryStatementFolder"
        }
      },
      "relationships": {
        "document": {
          "predicate": "ext:hasDocumentContainer",
          "target": "document-container",
          "cardinality": "one"
        },
        "published-version": {
          "type": "url",
          "predicate": "ext:publishedVersion",
          "target": "published-regulatory-attachment-container",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/reglement/"
    },
    "published-regulatory-attachment-containers": {
      "name": "published-regulatory-attachment-container",
      "class": "ext:PublishedRegulatoryAttachmentContainer",
      "attributes": {
        "valid-through": {
          "type": "datetime",
          "predicate": "schema:validThrough"
        }
      },
      "relationships": {
        "current-version": {
          "predicate": "ext:currentVersion",
          "target": "published-regulatory-attachment",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/published-regulatory-attachment-container/"
    },
    "published-regulatory-attachments": {
      "name": "published-regulatory-attachment",
      "class": "ext:PublishedRegulatoryAttachment",
      "relationships": {
        "content": {
          "predicate": "ext:content",
          "target": "file",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/published-regulatory-attachment/"
    },
    "skos-concepts": {
      "name": "skos-concept",
      "class": "skos:Concept",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "notation": {
          "type": "string",
          "predicate": "skos:notation"
        },
        "search-label": {
          "type": "string",
          "predicate": "ext:searchLabel"
        },
        "created-on": {
          "type": "datetime",
          "predicate": "pav:createdOn"
        },
        "position": {
          "type": "number",
          "predicate": "schema:position"
        }
      },
      "relationships": {
        "in-scheme": {
          "predicate": "skos:inScheme",
          "target": "concept-scheme",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://lblod.data.gift/concepts/"
    },
    "concept-schemes": {
      "name": "concept-scheme",
      "class": "skos:ConceptScheme",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "concepts": {
          "predicate": "skos:inScheme",
          "target": "skos-concept",
          "cardinality": "many",
          "inverse": true
        },
        "top-concepts": {
          "predicate": "skos:topConceptOf",
          "target": "concept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/concept-schemes/"
    },
    "concepts": {
      "name": "concept",
      "class": "ext:Concept",
      "attributes": {
        "valid": {
          "type": "boolean",
          "predicate": "ext:valid"
        }
      },
      "new-resource-base": "http://data.lblod.info/concepts/"
    },
    "mappings": {
      "name": "mapping",
      "class": "ext:Mapping",
      "attributes": {
        "variable": {
          "type": "string",
          "predicate": "ext:variable"
        },
        "type": {
          "type": "string",
          "predicate": "ext:variableType"
        }
      },
      "relationships": {
        "instruction": {
          "predicate": "ext:instructionVariable",
          "target": "template",
          "cardinality": "one"
        },
        "code-list": {
          "predicate": "ext:codeList",
          "target": "code-list",
          "cardinality": "one"
        },
        "expects": {
          "predicate": "ext:expects",
          "target": "shape",
          "cardinality": "one"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://data.lblod.info/mappings/"
    },
    "code-lists": {
      "name": "code-list",
      "class": "lblodmow:Codelist",
      "super": ["concept-scheme"],
      "attributes": {
        "created-on": {
          "type": "datetime",
          "predicate": "pav:createdOn"
        }
      },
      "relationships": {
        "type": {
          "predicate": "dct:type",
          "target": "skos-concept",
          "cardinality": "one"
        },
        "publisher": {
          "predicate": "dct:publisher",
          "target": "administrative-unit",
          "cardinality": "one"
        },
        "mappings": {
          "predicate": "ext:codeList",
          "target": "mapping",
          "cardinality": "many",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://lblod.data.gift/concept-schemes/"
    },
    "files": {
      "name": "file",
      "class": "nfo:FileDataObject",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "nfo:fileName"
        },
        "format": {
          "type": "string",
          "predicate": "dct:format"
        },
        "size": {
          "type": "number",
          "predicate": "nfo:fileSize"
        },
        "extension": {
          "type": "string",
          "predicate": "dbpedia:fileExtension"
        },
        "created": {
          "type": "datetime",
          "predicate": "nfo:fileCreated"
        }
      },
      "relationships": {
        "download": {
          "predicate": "nie:dataSource",
          "target": "file",
          "cardinality": "one",
          "inverse": true
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "http://data.example.com/files/"
    }
  }
}
